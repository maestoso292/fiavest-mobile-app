{
  "expo": {
    "name": "fiavest-mobile-app",
    "slug": "fiavest-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/fiavest-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#D3D3D3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fiavest.expo.googlesignin",
      "config" : {
        "googleSignIn" : {
          "reservedClientId": "com.googleusercontent.apps.6745347667-o1101gp7hd9j0d4gcfedi6ei7plmegsb"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "fiavestAndroid.expo.googlesignin",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
